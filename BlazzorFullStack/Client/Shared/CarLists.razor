@using BlazzorFullStack.Shared
@inject ICarService CarService
@implements IDisposable


@foreach(var car in CarService.Cars)
{
    <div class="">
        <a href="/car/@car.Id"><h2>@car.Brand @car.Model</h2></a>
        <h3>@car?.Category?.CarType</h3>
        <img src="data:image/jpeg;base64," />
        <h3>$@car?.Price</h3>
    </div>
}

@code {
    protected override void OnInitialized()
    {
        CarService.CarChanged += StateHasChanged;
    }
    public void Dispose()
    {
        CarService.CarChanged -= StateHasChanged;
    }
}
